.settingPage {
  min-height: 100%;
  background-color: #F1F4F6;
  padding: 30rpx 0;
  box-sizing: border-box;
}

.settingList {
  padding: 0 20rpx;
}

.settingItem {
  background-color: #ffffff;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
  padding: 24rpx 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.itemIconLeft {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.itemText {
  flex: 1;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.itemTitle {
  font-size: 30rpx;
  font-weight: 500;
  color: #111;
}

.itemIconRight {
  width: 24rpx;
  height: 24rpx;
}

